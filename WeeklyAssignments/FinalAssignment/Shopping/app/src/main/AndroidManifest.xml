<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.natjen.android.shopping">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/shopping_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/shopping_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".ShoppingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ListActivity"
            android:parentActivityName=".ShoppingActivity" />
        <activity
            android:name=".VersionActivity"
            android:parentActivityName=".ShoppingActivity" />
    </application>

</manifest>